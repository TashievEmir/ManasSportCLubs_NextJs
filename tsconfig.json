{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@/public": ["./public"],
      "@/src": ["./src"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "baseUrl": ".",
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/app-table/index.jsx", "src/components/prompt/Prompt.jsx", "src/app/signup/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
